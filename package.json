{
  "name": "mini-baby-grow",
  "version": "1.0.0",
  "description": "基于Mpx和vant开发的微信小程序应用，使用微信云开发作为服务端",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devbuild": "node ./build/build.js",
    "watch": "node ./build/build.js -w",
    "build": "node ./build/build.js -p",
    "prod": "node ./build/build.js -p",
    "watch:cross": "npx npm-run-all --parallel 'watch --wx --ali --swan --web' httpserver",
    "build:cross": "npm run build --wx --ali --swan --web",
    "prod:cross": "npm run prod --wx --ali --swan --web",
    "watchwebpure": "npm run watch --web",
    "watchweb": "npx npm-run-all --parallel watchwebpure httpserver",
    "httpserver": "npx http-server dist/web",
    "help": "node ./build/build.js --help"
  },
  "author": "winslow <wsw.sun@qq.com>",
  "license": "ISC",
  "dependencies": {
    "@mpxjs/api-proxy": "^2.3.0",
    "@mpxjs/core": "^2.4.0",
    "@mpxjs/fetch": "^1.0.5",
    "@vant/weapp": "^1.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.3"
  },
  "devDependencies": {
    "@mpxjs/webpack-plugin": "^2.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.0",
    "md5": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "ora": "^2.0.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "style-loader": "^1.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.4",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.2"
  }
}
